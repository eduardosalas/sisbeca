<h1>XUser list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Ci</th>
            <th>Login</th>
            <th>Password</th>
            <th>Admin</th>
            <th>Name</th>
            <th>Email</th>
            <th>Enabled</th>
            <th>Createdby</th>
            <th>Createdon</th>
            <th>Modifiedon</th>
            <th>Archivador</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('xuser_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.ci }}</td>
            <td>{{ entity.login }}</td>
            <td>{{ entity.password }}</td>
            <td>{{ entity.admin }}</td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.enabled }}</td>
            <td>{{ entity.createdby }}</td>
            <td>{% if entity.createdon %}{{ entity.createdon|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{% if entity.modifiedon %}{{ entity.modifiedon|date('Y-m-d H:i:s') }}{% endif%}</td>
            <td>{{ entity.archivador }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('xuser_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('xuser_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('xuser_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
