<h1>XUser</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Ci</th>
            <td>{{ entity.ci }}</td>
        </tr>
        <tr>
            <th>Login</th>
            <td>{{ entity.login }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ entity.password }}</td>
        </tr>
        <tr>
            <th>Admin</th>
            <td>{{ entity.admin }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Enabled</th>
            <td>{{ entity.enabled }}</td>
        </tr>
        <tr>
            <th>Createdby</th>
            <td>{{ entity.createdby }}</td>
        </tr>
        <tr>
            <th>Createdon</th>
            <td>{{ entity.createdon|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Modifiedon</th>
            <td>{{ entity.modifiedon|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Archivador</th>
            <td>{{ entity.archivador }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('xuser') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('xuser_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('xuser_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
